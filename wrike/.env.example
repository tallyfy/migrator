# Wrike API Configuration
WRIKE_API_KEY=your_wrike_api_key_here
WRIKE_BASE_URL=https://www.wrike.com/api/v4

# Tallyfy API Configuration
TALLYFY_API_KEY=your_tallyfy_key_here
TALLYFY_ORGANIZATION=your_organization_subdomain

# Migration Options
MIGRATE_ARCHIVED=false
MIGRATE_CUSTOM_FIELDS=true
MIGRATE_ATTACHMENTS=true
BATCH_SIZE=100

# Anthropic API for intelligent decisions (optional)
ANTHROPIC_API_KEY=sk-ant-api03-...
AI_MODEL=claude-3-haiku-20240307
AI_TEMPERATURE=0
AI_MAX_TOKENS=500

# AI Feature Flags
AI_ASSESS_WORKFLOW_COMPLEXITY=true
AI_MAP_CUSTOM_FIELDS=true
AI_OPTIMIZE_FOLDER_HIERARCHY=true
AI_TRANSFORM_DEPENDENCIES=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/migration.log