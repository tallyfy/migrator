# Kissflow to Tallyfy Migration Configuration
# Copy this file to .env and fill in your credentials

# Kissflow Configuration
# Your Kissflow subdomain (e.g., if your URL is https://mycompany.kissflow.com, enter "mycompany")
KISSFLOW_SUBDOMAIN=your_subdomain

# Your Kissflow account ID (found in Admin > Account Settings)
KISSFLOW_ACCOUNT_ID=your_account_id

# Kissflow Access Keys (create in Admin > API & Webhooks > Access Keys)
KISSFLOW_ACCESS_KEY_ID=your_access_key_id
KISSFLOW_ACCESS_KEY_SECRET=your_access_key_secret

# Tallyfy Configuration
# Your Tallyfy API token (create in Settings > API)
TALLYFY_API_TOKEN=your_api_token

# Your Tallyfy organization ID (found in Settings > Organization)
TALLYFY_ORGANIZATION_ID=your_organization_id

# Optional: Tallyfy API base URL (default: https://go.tallyfy.com/api)
# TALLYFY_API_BASE_URL=https://go.tallyfy.com/api

# Migration Options
# Maximum number of instances to migrate per template (default: 100)
# INSTANCE_LIMIT=100

# Enable debug logging (default: false)
# DEBUG=false

# Backup directory for checkpoints (default: ./backups)
# BACKUP_DIR=./backups